<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <parent>
    <artifactId>WattDepot-redesign</artifactId>
    <groupId>org.wattdepot3</groupId>
    <version>1.0.0</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.wattdepot3</groupId>
  <artifactId>wattdepot-client</artifactId>
  <name>wattdepot-client</name>
  <prerequisites>
    <maven>3.0.3</maven>
  </prerequisites>
  <dependencies>
    <dependency>
      <groupId>org.wattdepot3</groupId>
      <artifactId>wattdepot-core</artifactId>
      <version>1.0.0</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>httpunit</groupId>
      <artifactId>httpunit</artifactId>
      <version>1.7</version>
      <scope>test</scope>
      <exclusions>
        <exclusion>
          <artifactId>jtidy</artifactId>
          <groupId>jtidy</groupId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>4.11</version>
      <scope>test</scope>
      <exclusions>
        <exclusion>
          <artifactId>hamcrest-core</artifactId>
          <groupId>org.hamcrest</groupId>
        </exclusion>
      </exclusions>
    </dependency>
  </dependencies>
  <properties>
    <findbugs.filter.file>${config.dir}/findbugs.exclude.xml</findbugs.filter.file>
    <checkstyle.filter.file>${config.dir}/checkstyle.xml</checkstyle.filter.file>
    <config.dir>${basedir}/..</config.dir>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>
</project>

